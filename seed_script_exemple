job("$name") {
  description ('an example Jenkins job created with Job dsl')
  scm {
    git{
         remote{
                 url ('https://github.com/yossiazu/t1.git')
         }
      }
    }
  triggers {
    scm('* * * * *')
  }
  steps {
    shell(sh 'ls -la') 
    shell(sh 'echo job dsl work')
  }
}
